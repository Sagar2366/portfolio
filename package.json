{
  "private": true,
  "name": "raisedadead",
  "description": "Nothing fancy here, just one of my personal websites.",
  "license": "ISC",
  "author": "mrugesh mohapatra <me@raisedadead.com> (https://raisedadead.com)",
  "homepage": "/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raisedadead/raisedadead.git"
  },
  "bugs": {
    "url": "https://github.com/raisedadead/raisedadead/issues"
  },
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "build": "react-app-rewired build",
    "commit": "git-cz",
    "commit:retry": "git-cz --retry",
    "eject": "react-scripts eject",
    "lint": "lint-staged",
    "start": "react-app-rewired start",
    "test": "react-app-rewired test --env=jsdom"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "commitizen.config.js"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.6",
    "@fortawesome/react-fontawesome": "0.1.3",
    "better-react-spinkit": "^2.0.4",
    "classnames": "^2.2.6",
    "react": "^16.6.0",
    "react-app-rewire-styled-components": "^3.0.2",
    "react-dom": "^16.6.0",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.3.1",
    "styled-components": "^3.4.10",
    "uikit": "^3.0.0-beta.34"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "commitizen": "^3.0.4",
    "cz-customizable": "^5.2.0",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "prettier": "1.14.3",
    "prettier-package-json": "^2.0.1",
    "react-app-rewired": "^1.6.2",
    "react-scripts": "^2.0.5"
  },
  "engines": {
    "node": ">8"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ]
  }
}
